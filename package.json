{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt/ui": "^2.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pinia": "^2.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.1.15",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/image": "^1.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "5.4.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/components": "^10.5.0",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vitest": "^0.3.9",
    "happy-dom": "^12.10.3",
    "nuxt": "^3.8.1",
    "prisma": "^5.5.2",
    "release-it": "^16.2.1",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "tailwindcss": "^3.3.5",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    }
  },
  "prisma": {
    "schema": "server/database/schema.prisma",
    "seed": "tsx server/database/seed.ts"
  }
}
